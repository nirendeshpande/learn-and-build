<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.niren.com/base/bo/Student" 
xmlns:tns="http://www.niren.com/base/bo/Student" elementFormDefault="qualified">

	<element name="Student">      
        <complexType>
            <sequence>
                <element name="name" type="string"/>  
                <element name="age" type="int"/>                                            
                <element name="class" type="string"/>
            </sequence>
        </complexType>
    </element>
</schema>