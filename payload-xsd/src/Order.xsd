<?xml version="1.0" encoding="UTF-8"?>
<xs:schema version="1.0" targetNamespace="http://www.niren.com/base/bo/Order"  
 xmlns="http://www.niren.com/base/bo/Order" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  
    <xs:element name="Order">      
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int"/>                               
                <xs:element name="collection_size" type="xs:int"/>
                <xs:element name="order_type" type="xs:string"/>         
                <xs:element name="quantity" type="xs:int"/>
                <xs:element name="price" type="xs:int"/>
                <xs:element name="description" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>